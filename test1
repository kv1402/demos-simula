
        ! ref(BIN) FIFO;
        ref(WaitQ) queue;
        ref(Tally) delay,sessionLength;
        ref(Rdist) Send, serve;
        int Ptot;
        
        Entity class User(Ptot, Send, buffer);
            begin
                integer Pi;
                Pi := 1;
                SessionStart:= time;
                A:
                        if Pi < Ptot then begin
                                new Packet(edit("packet",Pi)).schedule(now);
                                hold(Send.sample); 
                                Goto A;
                        end;
                        
                buffer.take(1000);
                SessionLength.update(time-SessionStart);
                 end;


         Entity class Packet();
            begin
                ref(BIN) buffer;
                timestamp:= time;
                queue.wait
                
                delay.update(time-timestamp)
                buffer.give(1);
            end;


        Entity class Network(queue,serve);
        begin
                ref(Packet) packet;
                packet:= queue.coopt;
                hold(serve.sample);
        end;
        
        setseed(33427485);

        Send:-new NegExp("Sending intensity", 0.1);
        Serve:- new NegExp("Serving intensity", 1);


        delay:- new Tally("Totalt time in queue");
        sessionLength:- new Tally("Total session length");




        numreplica:=20;

        WHILE replication LE numreplica DO
        begin


            totalDelay.reset;


            User1:- new User("User1", Send);
            User2:- new User("User2", Send);
            User3:- new User("User3", Send);    
            
            Pi:- new Count("Count Input");
            Po:- new Count("Count Output");


            User1.schedule(0);
            User2.schedule(0);
            User3.schedule(0);
            
    

            New Server("Server").schedule(0);

            Hold(60*60*24);

            totalUp:=totalUp+upTime.sum;
            totalDown:=totalDown+downTime.sum;

            Tilgjengelig.update(upTime.sum/(upTime.sum+downTime.sum));

            if numreplica > 2 then noreport;

            replicate;
        end;

        if numreplica < 3 then norepli;


        avgPPdelay:= totalDelay/Ptot/3;
        avgSession:= sessionLenght/3;

        OutText("Average sessionLenth");outReal(avgSession,10);OutImage;
        OutText("Average delay per packet");outReal(avgPPdelay, 4, 10);OutImage;
    end;
end;

